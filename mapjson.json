{
	"name": "MAPS_TEST",
	"imports": ["datasource"],
	"description": "SAC Test Maps",
	"newInstancePrefix": "MAPS_TEST",
	"eula": "",
	"vendor": "Contigo Consulting AG - MKoshy, JBuergmayr",
	"license": "",
	"id": "MAPS_TEST",
	"version": "1.0.3",
	"icon": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "custom-maps-test",
			"url": "https://mridul0007.github.io/map.js", 
			"integrity": ""	,
			"ignoreIntegrity": true
			
		}		
	],"properties": {
		"p_gmap_key":{
			"type" : "string"
		},
		"coordinate_master_map_data": {
			"type": "ResultSet[]"
		},
		"coordinate_master_table_data": {
			"type": "ResultSet[]"
		},
        "default_map": {
			"type": "string"
		},
		"read_finish": {
			"type": "boolean"
		},
		"reset_data": {
			"type": "boolean"
		},
		"icons_id":{
			"type": "Selection"
		}	
	},
	"methods": {
		"set_google_mapsjs_api_key": {
			"description": "set credentials",
			"parameters": [
				{
					"name": "p_gmap_key",
					"type": "string",
					"description": "Google Map API key to be set"
				}
			]
		},
		"set_default_map": {
			"description": "set default map",
            "parameters": [
				{
					"name": "default_map",
					"type": "string",
					"description": "set default map"
				}]
			},    
		"set_coordinate_master_data":{
			"description": "set coordinate master data",
			"parameters": [
				{
					"name": "coordinate_master_map_data",
					"type": "ResultSet[]",
					"description": "coordinate master data"
				},
				{
					"name": "read_finish",
					"type": "boolean",
					"description": "flag for read coordinate master data "
				},
				{
					"name": "reset_data",
					"type": "boolean",
					"description": "flag for new coordinate master data read"
				}
			]
		},
		"set_coordinate_master_table_data":{
			"description": "set coordinate master table data",
			"parameters": [
				{
					"name": "coordinate_master_table_data",
					"type": "ResultSet[]",
					"description": "coordinate master table data"
				}
			]
		},
		"set_icons_id":{
			"description": "set Icons for the Map",
			"parameters":[
				{
					"name": "icons_id",
					"type": "Selection",
					"description": "icons ids"
				}
			]
		}

	},
	"events": {
		"EVENTW2S_DB_FILL_COORDINATE_DATA": {
			"description": "read coordinate master data"
		},
		"EVENTW2S_DB_FILL_TABLE_DATA": {
			"description": "read table data"
		}
	}
	
}
